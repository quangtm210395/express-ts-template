{
  "compilerOptions": {
    "esModuleInterop": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "pretty": true,
    "types": [
      "node"
    ],
    "sourceMap": true,
    "target": "es5",
    "baseUrl": ".",
    "outDir": "dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
  },
  "exclude": [
    "node_modules/*",
    "test"
  ]
}
